<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.databindingsamples">
    <uses-permission android:name="android.permission.INTERNET"/>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".samples.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".samples.viewid.ViewWithIdActivity"
            android:label="ViewWithIdActivity" />
        <activity
            android:name=".samples.basic.BasicActivity"
            android:label="BasicActivity" />
        <activity
            android:name=".samples.custombinding.CutomBindingActivity"
            android:label="CutomBindingActivity" />
        <activity
            android:name=".samples.includes.IncludesActivity"
            android:label="IncludesActivity" />
        <activity
            android:name=".samples.resource.ResourceActivity"
            android:label="ResourceActivity" />
        <activity
            android:name=".samples.collections.CollectionsActivity"
            android:label="CollectionsActivity" />
        <activity
            android:name=".samples.observable.ObservableActivity"
            android:label="ObservableActivity" />
        <activity
            android:name=".samples.viewstub.ViewStubActivity"
            android:label="ViewStubActivity" />
        <activity android:name=".samples.dynamic.DynamicActivity"
            android:label="DynamicActivity"/>
        <activity android:name=".samples.attributesetter.AutoAttributeSetterActivity"
            android:label="AutoAttributeSetterActivity"/>
        <activity android:name=".samples.converters.ConvertersActivity"
            android:label="ConvertersActivity"/>
    </application>

</manifest>